{
  "name": "es6-way",
  "version": "0.4.0",
  "homepage": "https://cham11ng.github.io/webpack-starter",
  "description": "Webpack Starter",
  "repository": {
    "type": "git",
    "url": "git@github.com:cham11ng/webpack-starter.git"
  },
  "author": {
    "name": "Sagar Chamling",
    "email": "sgr.raee@gmail.com"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpackbar": "^1.1.3"
  },
  "scripts": {
    "prepush": "yarn build",
    "precommit": "lint-staged",
    "lint:fix": "eslint --fix src/**/*.{js}",
    "prettify": "prettier --write src/**/*.{js,json}",
    "start": "NODE_ENV=development webpack-dev-server --open",
    "dev": "NODE_ENV=development webpack",
    "watch": "NODE_ENV=development webpack --watch",
    "build": "NODE_ENV=production webpack --hide-modules"
  },
  "lint-staged": {
    "linters": {
      "*.{ts, js}": [
        "lint:fix",
        "prettify",
        "git add"
      ]
    }
  },
  "keywords": [
    "es6",
    "node",
    "sass",
    "webpack4",
    "javascript"
  ],
  "private": true,
  "license": "MIT"
}
